ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/GrowingTextView" "$PODS_CONFIGURATION_BUILD_DIR/MBAutoGrowingTextView"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/GrowingTextView/GrowingTextView.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/MBAutoGrowingTextView/MBAutoGrowingTextView.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "GrowingTextView" -framework "MBAutoGrowingTextView"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
